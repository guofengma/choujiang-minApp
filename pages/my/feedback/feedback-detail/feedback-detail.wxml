<top-bar/>
<view class='feedback-detail'>
  <view class='font-26 title flexR-sb'>
    问题编号：{{list.code}} 物流问题
  </view>

  <view class='userInfo'>
    <view class='group'>
      <text class='gray'>您的姓名：</text>
      <view >{{list.name}}</view>
    </view>
    <view class='group'>
      <text class='gray'>联系电话：</text>
      <view >{{list.telphone}}</view>
    </view>
  </view>

  <view class=''>
    <view class='red font-28'>解决反馈问题</view>
    <view class='grayBg content'>
      {{list.answer}}
    </view>
  </view>
</view>